{
  "manifest_version": 2,

  "name": "Practice Plugin",
  "description": "This is a practice extension",
  "version": "1.0",
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "browser_action": {
   "default_icon": "images/bubble.png",
   "default_popup": "popup.html"
  },
  "permissions": [
     "activeTab",
      "<all_urls>"
  ]
}
